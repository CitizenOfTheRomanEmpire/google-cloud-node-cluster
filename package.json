    "chai-http": "^3.0.0",
    "eslint": "^4.11.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.2.0",
    "istanbul": "^0.4.5",
    "mocha": "^4.0.1",
    "nodemon": "^1.11.0",
    "nsp": "^3.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/RomansBermans/google-cloud-node-cluster.git"
  },
