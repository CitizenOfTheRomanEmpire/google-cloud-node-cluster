{
  "name": "google-cloud-node-cluster",
  "version": "0.0.0-alpha.0",
  "main": "server.js",
  "scripts": {
    "start": "node ${SERVICE:-.}",

    "develop": "nodemon .",

    "pretest": "eslint --ext .js .",
    "test": "NODE_ENV=test istanbul cover _mocha -- test/server -R spec --recursive --ui bdd --slow 100 --timeout 1000 --exit",
